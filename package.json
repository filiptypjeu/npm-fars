{
  "name": "fars",
  "version": "3.1.0",
  "description": "NPM package for using FARS.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "deploy": "bash deploy.sh",
    "dev": "nodemon src/index.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "lint-fix": "tslint -p tsconfig.json --fix",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run format && npm run lint-fix",
    "test": "jest --config jestconfig.json --coverage",
    "test-no-coverage": "jest --config jestconfig.json",
    "version": "npm run format && git add -A src"
  },
  "author": "Filip Stenbacka",
  "license": "ISC",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/moment": "^2.13.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:filiptypjeu/npm-fars.git"
  },
  "homepage": "https://github.com/filiptypjeu/npm-fars",
  "dependencies": {
    "moment": "^2.29.1",
    "web-login-manager": "^1.0.2"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "fars",
    "teknologf√∂reningen",
    "TF"
  ]
}
